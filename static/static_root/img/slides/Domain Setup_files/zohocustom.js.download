		function decodeHexString(str) {
			str = str.replace(/\\x([0-9A-Fa-f]{2})/g, function() {
				return String.fromCharCode(parseInt(arguments[1], 16));
			});
			str = str.replace(/\\u([\d\w]{4})/gi, function() {
				return String.fromCharCode(parseInt(arguments[1], 16));
			});
			return str;
		}
						
