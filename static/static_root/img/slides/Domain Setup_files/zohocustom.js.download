function encodeFun(url){
	url = decodeURI(url);
	if(url.indexOf('#') > -1){
		var _length = url.split('#').length,  _addUrl = '';
		for(var i=1 ;i<_length; i++){
			_addUrl+= '#'+url.split('#')[i]
		}
		url = url.split('#')[0]+''+encodeURI(_addUrl);
	}
	if(url.indexOf('?') > -1){
		var _length = url.split('?').length,  _addUrl = '';
		for(var i=1 ;i<_length; i++){
			_addUrl+= '?'+url.split('?')[i]
		}
		url = url.split('?')[0]+''+encodeURI(_addUrl);
	}
	return url;
}
		
