        if (UserAgent.is.chrome() !== true && UserAgent.is.firefox() !== true) {
            return false;
        }
        if (Notification === undefined || Notification === null) {
            return false;
        }
        if (Notification.permission === 'granted') {
            return false;
        }
        if (Notification.permission === 'denied') {
            return false;
