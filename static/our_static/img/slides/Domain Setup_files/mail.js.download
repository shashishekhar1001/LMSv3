jQuery(document).ready(function(){
    if($('body').hasClass('node-tnid-22820')){

		if(domainOne=="eu"){
			changeCurrency('euro');
		}
		else{
			if(CountryCode == "IN"){
				changeCurrency('inr');
			} else if(CountryCode == "DE"){
				changeCurrency('euro');
                        } else{
				changeCurrency('usd');
                        }
		}
    }
});
    function changeCurrency(e){
		$('.priceChangeAmt').each(function(index, element) {
                        var inrAmt=$(this).attr('data-inr');
			var usdAmt=$(this).attr('data-usd');
			var poundsAmt=$(this).attr('data-pounds');
			if(e=='inr'){
				$('.priceChangetxt').text('₹');
				$(this).text(inrAmt);
			}
			else if(e=='usd'){
				$('.priceChangetxt').text('$');
				$(this).text(usdAmt);
			}
			else if(e=='euro'){
				$('.priceChangetxt').text('€');
				$(this).text(usdAmt);
			}
			else if(e=='pound'){
				$('.priceChangetxt').text('£');
				$(this).text(poundsAmt);
			}
        });
    }

