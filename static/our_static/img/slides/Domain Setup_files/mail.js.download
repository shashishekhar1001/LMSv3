            } else if (state == $.fn.zaSignUp.SIGNUP_STATE.SIGNUP_COMPLETED) {
                try {
                    $zoho.livedesk.visitor.customaction("Signed Up");
                } catch (exp) {}
            }
        },
        handleConfirmation: function(data) {

            data.doAction();

        }

    });


    $("#zmailSignup input:first").focus();

    afterSignUpPageLoaded();
}

